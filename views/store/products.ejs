                                    <div id="inc">
                                        <button onclick="increment(0)" id="plus">+</button>
                                        <input id="demoInput" class="insider" class="inp" type=text min="1" value="1">
                                        <button onclick="decrement(0)" id="plus">-</button>
                                    </div>
                                </th>
                            </tr>
                        </thead>
                        <thead>
                            <tr class="d-flex" id="white">
                                <th class="col-2">2</th>
                                <th class="col">Puma Unisex sneakers</th>
                                <th class="col-2 quant">
                                    <div id="inc">
                                        <button onclick="increment(1)" id="plus">+</button>
                                        <input id="demoInput" class="insider" type=text min="1" value="1">
                                        <button onclick="decrement(1)" id="plus">-</button>
                                    </div>
                                </th>
                            </tr>
                        </thead>
                        <thead>
                            <tr class="d-flex" id="blue">
                                <th class="col-2">3</th>
