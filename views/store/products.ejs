                                        <button onclick="increment(2)" id="plus">+</button>
                                        <input id="demoInput" class="insider" type=text min="1" value="1">
                                        <button onclick="decrement(2)" id="plus">-</button>
                                    </div>
                                </th>
                            </tr>
                        </thead>
                        <thead>
                            <tr class="d-flex" id="white">
                                <th class="col-2">4</th>
