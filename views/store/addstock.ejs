            <script>
              function increment(i) {
                var x = document.getElementsByClassName('insider')[i].value;
                x++;
                document.getElementsByClassName('insider')[i].value = x;
              }
              function decrement(i) {
                var x = document.getElementsByClassName('insider')[i].value;
                if (x != 0) { x--; }
                document.getElementsByClassName('insider')[i].value = x;
              }

            </script>
